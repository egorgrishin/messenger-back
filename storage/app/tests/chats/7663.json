[[2,"\u0414\u043e\u0431\u0440\u044b\u0439 \u0432\u0435\u0447\u0435\u0440\ud83d\ude0c"],[1,"\u041f\u0440\u0438\u0432\u0435\u0442, \u043a\u0430\u043a \u043d\u0430\u0441\u0442\u0440\u043e\u0435\u043d\u0438\u0435?"],[2,"\u0428\u0438\u043a\u0430\u0440\u043d\u043e\u0435, \u0447\u0438\u0442\u0430\u044e \u043e\u0447\u0435\u0440\u0435\u0434\u043d\u0443\u044e \u043a\u043d\u0438\u0433\u0443, \u044f \u043f\u043e\u043b\u0438\u0433\u043b\u043e\u0442 \u0435\u0449\u0451 \u0442\u043e\u0442\ud83d\ude05 \u0412\u044b \u0447\u0435\u043c \u0437\u0430\u043d\u0438\u043c\u0430\u0435\u0442\u0435\u0441\u044c, \u0443\u0432\u043b\u0435\u043a\u0430\u0435\u0442\u0435\u0441\u044c?"],[1,"\u0417\u0430\u043a\u043e\u043d\u0447\u0438\u043b \u043f\u0438\u0441\u0430\u0442\u044c \u043f\u043e\u0441\u0442 \u043e \u0441\u0442\u0430\u0440\u044b\u0445 \u043a\u0432\u0430\u0440\u0442\u0438\u0440\u0430\u0445 \u041f\u0435\u0442\u0435\u0440\u0431\u0443\u0440\u0433\u0430, \u0436\u0435\u043b\u0430\u044e \u0441\u0435\u0440\u0438\u044e \u043f\u043e\u0441\u0442\u043e\u0432 \u0434\u043b\u044f \u0441\u0432\u043e\u0438\u0445 \u043f\u043e\u0434\u043f\u0438\u0441\u0447\u0438\u043a\u043e\u0432 \u043e \u0441\u0442\u0430\u043b\u0438\u043d\u043a\u0430\u0445. \u0414\u0430 \u0441\u043e\u0431\u0441\u0442\u0432\u0435\u043d\u043d\u043e \u044d\u0442\u0438\u043c \u0438 \u0437\u0430\u043d\u0438\u043c\u0430\u044e\u0441\u044c. \u0423\u0436\u0435 \u0431\u043e\u043b\u044c\u0448\u0435 \u043c\u0438\u043b\u043b\u0438\u043e\u043d\u0430\u043f\u043e\u0434\u043f\u0438\u0441\u0430\u043b\u043e\u0441\u044c)"],[1,"\u0417\u0430\u043a\u043e\u043d\u0447\u0438\u043b \u043f\u0438\u0441\u0430\u0442\u044c \u043f\u043e\u0441\u0442 \u043e \u0441\u0442\u0430\u0440\u044b\u0445 \u043a\u0432\u0430\u0440\u0442\u0438\u0440\u0430\u0445 \u041f\u0435\u0442\u0435\u0440\u0431\u0443\u0440\u0433\u0430, \u0434\u0435\u043b\u0430\u044e \u0441\u0435\u0440\u0438\u044e \u043f\u043e\u0441\u0442\u043e\u0432 \u0434\u043b\u044f \u0441\u0432\u043e\u0438\u0445 \u043f\u043e\u0434\u043f\u0438\u0441\u0447\u0438\u043a\u043e\u0432 \u043e \u0441\u0442\u0430\u043b\u0438\u043d\u043a\u0430\u0445. \u0414\u0430 \u0441\u043e\u0431\u0441\u0442\u0432\u0435\u043d\u043d\u043e \u044d\u0442\u0438\u043c \u0438 \u0437\u0430\u043d\u0438\u043c\u0430\u044e\u0441\u044c. \u0423\u0436\u0435 \u0431\u043e\u043b\u044c\u0448\u0435 \u043c\u0438\u043b\u043b\u0438\u043e\u043d\u0430\u043f\u043e\u0434\u043f\u0438\u0441\u0430\u043b\u043e\u0441\u044c)"],[2,"\u0412\u0430\u043c \u0441\u043a\u043e\u043b\u044c\u043a\u043e \u043b\u0435\u0442? \u0411\u044b\u043b\u0438 \u0432 \u0431\u0440\u0430\u043a\u0435? \u0415\u0441\u0442\u044c \u0434\u0435\u0442\u0438?"],[1,"\u041a\u0430\u043a\u043e\u0439 \u043f\u0440\u044f\u043c \u0434\u043e\u043f\u0440\u043e\u0441 )) \u0435\u0441\u043b\u0438 \u0447\u0442\u043e, \u044f \u043d\u0430\u043b\u043e\u0433\u0438 \u043f\u043b\u0430\u0447\u0443, \u043d\u043e\u0442\u044b \u0436\u0435 \u043d\u0435 \u0438\u0437 \u043d\u0430\u043b\u043e\u0433\u043e\u0432\u043e\u0439 ?)) \u043c\u043d\u0435 \u0437\u0430 30, \u0441\u0435\u043c\u0435\u0439\u043d\u043e\u0435 \u043f\u043e\u043b\u043e\u0436\u0435\u043d\u0438\u0435- \u0435\u0441\u0442\u044c \u043a\u043e\u0442. \u0410 \u0442\u044b? \u041d\u0438\u0447\u0435\u0433\u043e \u0447\u0442\u043e \u044f \u043d\u0430 \u0442\u044b?"],[2,"\u041d\u0438\u0447\u0435\u0433\u043e \ud83d\ude0c \u042f \u0431\u044b\u043b \u0436\u0435\u043d\u0430\u0442 \u0442\u0440\u0438\u0436\u0434\u044b \ud83d\ude05\ud83d\ude05 \u041d\u0435 \u0441\u043b\u043e\u0436\u0438\u043b\u043e\u0441\u044c \u0432 \u043e\u0431\u0449\u0435\u043c\u041d\u0435\u0442 \u0436\u0438\u0432\u043e\u0442\u043d\u044b\u0445, \u043d\u043e \u0436\u0438\u0437\u043d\u044c \u044f \u043b\u044e\u0431\u043b\u044e \u0410 \u043e\u0441\u043e\u0431\u0435\u043d\u043d\u043e \u043b\u044e\u0431\u043b\u044e \u043f\u0443\u0442\u0435\u0448\u0435\u0441\u0442\u0432\u043e\u0432\u0430\u0442\u044c\u0432 \u0442\u0451\u043f\u043b\u044b\u0435 \u0441\u0442\u0440\u0430\u043d\u044b, \u043e\u0431\u043e\u0436\u0430\u044e \u0442\u0451\u043f\u043b\u044b\u0439 \u043a\u043b\u0438\u043c\u0430\u0442 \u0410 \u0432\u044b"],[2,"\u0422\u044b\ud83d\ude05"],[1,"\u042f \u043b\u044e\u0431\u043b\u044e \u043d\u0430 \u043f\u0430\u0440\u0443 \u043c\u0435\u0441\u044f\u0446\u0435\u0432 \u0443\u0435\u0437\u0436\u0430\u0435\u0442 \u0438\u0437 \u0447\u0435\u043b\u043e\u0432\u0435\u0439\u043d\u0438\u043a\u0430 \u0441\u0432\u043e\u0435\u0433\u043e, \u043d\u0430\u0440\u043e\u0434\u0443 \u0442\u0443\u0442 \u043a\u0443\u0447\u0430, \u0438\u043d\u043e\u0433\u0434\u0430 \u0445\u043e\u0447\u0435\u0442\u0441\u044f \u0443\u0435\u0434\u0438\u043d\u0435\u043d\u0438\u044f. \u0422\u0430\u043a \u0447\u0442\u043e\u043f\u043e\u0447\u0442\u0438 \u043a\u0430\u0436\u0434\u0443\u044e \u0437\u0438\u043c\u0443 \u044f \u0443\u0435\u0437\u0436\u0430\u044e \u043d\u0430 \u043e\u0441\u0442\u0440\u043e\u0432, \u0431\u043b\u0430\u0433\u043e \u0431\u043b\u043e\u0433\u0433\u0435\u0440- \u043f\u0440\u043e\u0444\u0435\u0441\u0441\u0438\u044f \u0443\u0434\u0430\u043b\u0451\u043d\u043d\u0430\u044f. \u0412\u0435\u0434\u0443 \u0442\u0430\u043c \u0432\u0435\u0431\u0438\u043d\u0430\u0440\u044b, \u0438\u043d\u043e\u0433\u0434\u0430 \u0443\u0441\u0442\u0440\u0430\u0438\u0432\u0430\u044e\u0432\u0441\u0442\u0440\u0435\u0447\u0438 \u0441 \u043f\u043e\u0434\u043f\u0438\u0441\u0447\u0438\u043a\u0430\u043c\u0438 \u0431\u043e\u043b\u044c\u0448\u0435 \u0432\u0441\u0435\u0433\u043e \u043b\u044e\u0431\u043b\u044e \u0422\u0430\u0438\u043b\u0430\u043d\u0434, \u043e\u043d \u0441\u0430\u043c\u044b\u0439 \u0446\u0438\u0432\u0438\u043b\u0438\u0437\u043e\u0432\u0430\u043d\u043d\u044b\u0439"],[1,"\u042f \u043b\u044e\u0431\u043b\u044e \u043d\u0430 \u043f\u0430\u0440\u0443 \u043c\u0435\u0441\u044f\u0446\u0435\u0432 \u0443\u0435\u0437\u0436\u0430\u0442\u044c \u0438\u0437 \u0447\u0435\u043b\u043e\u0432\u0435\u0439\u043d\u0438\u043a\u0430 \u0441\u0432\u043e\u0435\u0433\u043e, \u043d\u0430\u0440\u043e\u0434\u0443 \u0442\u0443\u0442 \u043a\u0443\u0447\u0430, \u0438\u043d\u043e\u0433\u0434\u0430 \u0445\u043e\u0447\u0435\u0442\u0441\u044f \u0443\u0435\u0434\u0438\u043d\u0435\u043d\u0438\u044f. \u0422\u0430\u043a \u0447\u0442\u043e\u043f\u043e\u0447\u0442\u0438 \u043a\u0430\u0436\u0434\u0443\u044e \u0437\u0438\u043c\u0443 \u044f \u0443\u0435\u0437\u0436\u0430\u044e \u043d\u0430 \u043e\u0441\u0442\u0440\u043e\u0432, \u0431\u043b\u0430\u0433\u043e \u0431\u043b\u043e\u0433\u0433\u0435\u0440- \u043f\u0440\u043e\u0444\u0435\u0441\u0441\u0438\u044f \u0443\u0434\u0430\u043b\u0451\u043d\u043d\u0430\u044f. \u0412\u0435\u0434\u0443 \u0442\u0430\u043c \u0432\u0435\u0431\u0438\u043d\u0430\u0440\u044b, \u0438\u043d\u043e\u0433\u0434\u0430 \u0443\u0441\u0442\u0440\u0430\u0438\u0432\u0430\u044e\u0432\u0441\u0442\u0440\u0435\u0447\u0438 \u0441 \u043f\u043e\u0434\u043f\u0438\u0441\u0447\u0438\u043a\u0430\u043c\u0438 \u0431\u043e\u043b\u044c\u0448\u0435 \u0432\u0441\u0435\u0433\u043e \u043b\u044e\u0431\u043b\u044e \u0422\u0430\u0438\u043b\u0430\u043d\u0434, \u043e\u043d \u0441\u0430\u043c\u044b\u0439 \u0446\u0438\u0432\u0438\u043b\u0438\u0437\u043e\u0432\u0430\u043d\u043d\u044b\u0439"],[1,"\u0410 \u043a\u0435\u043c \u0440\u0430\u0431\u043e\u0442\u0430\u0435\u0448\u044c?"],[2,"\u0412\u0435\u0440\u0438\u0448\u044c \u0432 \u041d\u041b\u041e?\ud83d\ude05"],[1,"\u041d\u0435 \u043e\u0441\u043e\u0431\u043e) \u0445\u043e\u0442\u044f \u0434\u0435\u043d\u044c\u0433\u0438 \u0442\u0430\u043a \u0431\u044b\u0441\u0442\u0440\u043e \u043a\u043e\u043d\u0447\u0430\u044e\u0442\u0441\u044f, \u0447\u0442\u043e \u043c\u043d\u0435\u043a\u0430\u0436\u0435\u0442\u0441\u044f \u0438\u0445 \u043a\u0442\u043e- \u0442\u043e \u043a\u0440\u0430\u0434\u0435\u0442. \u041c\u043e\u0436\u0435\u0442, \u043f\u0440\u0438\u0448\u0435\u043b\u044c\u0446\u044b? \ud83d\ude02\ud83d\ude02"],[2,"\u041e\u0439, \u043d\u0430 \u0444\u043e\u0442\u043e\u0431\u0430\u043d\u043a\u0430\u0445 \u043f\u0440\u043e\u0434\u0430\u044e \u0440\u0430\u0431\u043e\u0442\u044b, \u044f \u0444\u043e\u0442\u043e\u0433\u0440\u0430\u0444 \u041f\u043e\u044d\u0442\u043e\u043c\u0443\u043f\u0443\u0442\u0435\u0448\u0435\u0441\u0442\u0432\u0438\u044f \u0432\u0434\u0432\u043e\u0439\u043d\u0435 \u043f\u0440\u0438\u044f\u0442\u043d\u0435\u0439, \u043a\u043e\u0433\u0434\u0430 \u0438\u043c\u0435\u044e \u0437\u0430\u0440\u0430\u0431\u043e\u0442\u043e\u043a"],[2,"\u0412\u043e\u0442 \u044f \u0432\u0438\u0434\u0435\u043b \u041d\u041b\u041e, \u043a\u0430\u0436\u0435\u0442\u0441\u044f \ud83d\ude05\ud83d\ude05"],[2,"\u0427\u0442\u043e \u0435\u0449\u0451 \u0440\u0430\u0441\u0441\u043a\u0430\u0436\u0435\u0448\u044c?)"],[1,"\u0417\u0434\u043e\u0440\u043e\u0432\u043e! \u042f \u0440\u0430\u043d\u044c\u0448\u0435 \u0434\u043b\u044f \u0431\u043b\u043e\u0433\u0430 \u0441\u043d\u0438\u043c\u0430\u043b \u043d\u0430 \u0437\u0435\u0440\u043a\u0430\u043b\u0443, \u043d\u043e\u0443\u0441\u0442\u0430\u043b \u0435\u0451 \u0442\u0430\u0441\u043a\u0430\u0442\u044c)"],[1,"\u0410 \u0433\u0434\u0435 \u041d\u041b\u041e \u0442\u0435\u0431\u0435 \u0432\u0441\u0442\u0440\u0435\u0442\u0438\u043b\u043e\u0441\u044c?"],[2,"\u041d\u0435 \u043f\u043e\u0432\u0435\u0440\u0438\u0448\u044c, \u0432 \u0418\u043d\u0434\u0438\u0438"],[1,"\u0425\u0430\u0445\u0430, \u043d\u0438\u0447\u0435\u0433\u043e \u0441\u0435\u0431\u0435"],[1,"\u0412 \u0432\u0430\u0440\u0430\u043d\u0430\u0441\u0438?"],[1,"\u0422\u0430\u043c \u0441\u0430\u043c\u0430\u044f \u043b\u0443\u0447\u0448\u0430\u044f \u0430\u0442\u043c\u043e\u0441\u0444\u0435\u0440\u0430 \u0434\u043b\u044f \u041d\u041b\u041e"],[2,"\u0412 \u0425\u0430\u0439\u0434\u0430\u0440\u0430\u0431\u0430\u0434\u0435"],[2,"\u0421 \u0442\u043e\u0431\u043e\u0439 \u043f\u0440\u0438\u044f\u0442\u043d\u043e \u0431\u044b\u043b\u043e \u043f\u043e\u043e\u0431\u0449\u0430\u0442\u044c\u0441\u044f \ud83d\ude0c"]]